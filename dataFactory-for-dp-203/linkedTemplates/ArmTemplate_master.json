{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dataFactory-for-dp-203"
		},
		"linked_srv_az_sql_srv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linked_srv_az_sql_srv'"
		},
		"linkedsrv_for_storageaccthotfordp203_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedsrv_for_storageaccthotfordp203'"
		},
		"linkedsrv_for_storageaccthotfordp203_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccthotfordp203.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dataFactory-for-dp-203_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linked_srv_az_sql_srv_connectionString": {
						"value": "[parameters('linked_srv_az_sql_srv_connectionString')]"
					},
					"linkedsrv_for_storageaccthotfordp203_accountKey": {
						"value": "[parameters('linkedsrv_for_storageaccthotfordp203_accountKey')]"
					},
					"linkedsrv_for_storageaccthotfordp203_properties_typeProperties_url": {
						"value": "[parameters('linkedsrv_for_storageaccthotfordp203_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}