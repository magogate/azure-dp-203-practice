{
	"name": "df_copy_data_from_files_to_HR_db",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "csv_dataset_for_hr_db_files",
						"type": "DatasetReference"
					},
					"name": "srcHrFiles"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dataset_az_sql_db_hr",
						"type": "DatasetReference"
					},
					"name": "tgtEmployees"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EMPLOYEE_ID as string,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          EMAIL as string,",
				"          PHONE_NUMBER as string,",
				"          HIRE_DATE as string,",
				"          JOB_ID as string,",
				"          SALARY as string,",
				"          COMMISSION_PCT as string,",
				"          MANAGER_ID as string,",
				"          DEPARTMENT_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> srcHrFiles",
				"srcHrFiles sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EMPLOYEE_ID as integer,",
				"          FIRST_NAME as string,",
				"          LAST_NAME as string,",
				"          EMAIL as string,",
				"          PHONE_NUMBER as string,",
				"          HIRE_DATE as timestamp,",
				"          JOB_ID as string,",
				"          SALARY as integer,",
				"          COMMISSION_PCT as decimal(9,2),",
				"          MANAGER_ID as integer,",
				"          DEPARTMENT_ID as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          FIRST_NAME,",
				"          LAST_NAME,",
				"          EMAIL,",
				"          PHONE_NUMBER,",
				"          JOB_ID,",
				"          MANAGER_ID,",
				"          DEPARTMENT_ID",
				"     )) ~> tgtEmployees"
			]
		}
	}
}